alias ls='ls --color=auto'
alias grep='grep --color=auto'
alias g='git'
alias ga='git add'
alias gb='git branch -vv'
alias gco='git checkout'
alias gm='git commit -m'
alias gd='git diff'
alias gds='git diff --staged'
alias gh='git hist'
alias gl='git log'
alias gff='git merge --ff-only @{u}'
alias gpu='git pull'
alias gp='git push'
alias gr='git reset'
alias gs='git status'
alias gst='git status'
